{
  "name": "ludensify",
  "version": "0.0.1",
  "description": "Utility helpers and functions to make developing fun and easy",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "demo": "webpack",
    "prepublish": "npm run build && npm run demo",
    "demo:watch": "webpack-dev-server --hot --progress --colors",
    "precommit": "lint-staged",
    "build": "NODE_ENV=production cross-env BABEL_ENV=commonjs babel src --out-dir lib"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludens-reklamebyra/ludensify.git"
  },
  "keywords": [
    "utility",
    "helpers"
  ],
  "authors": [
    "Ã˜yvind Hermansen",
    "Shallan Paul",
    "Kristian Normand"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.2.3",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0",
    "prettier": "^0.22.0",
    "react-test-renderer": "^15.4.2",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  }
}
